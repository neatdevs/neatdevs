coroutine.resume(coroutine.create(function()
while true do
Swait()
WEAPONGUI.Mode.BackgroundColor3 = Color3.fromRGB(0+178*sick.PlaybackLoudness/1000,0,0)
WEAPONGUI.owo.BackgroundColor3 = C3(0,math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1))
WEAPONGUI.uwu.BackgroundColor3 = Color3.fromRGB(0,0,0+178*sick.PlaybackLoudness/1000)
WEAPONGUI.Vis.Size = UD2(0-sick.PlaybackLoudness/1400,0,0,16)
WEAPONGUI.Vis2.Size = UD2(0-sick.PlaybackLoudness/1200,0,0,16)
WEAPONGUI.Vis3.Size = UD2(0-sick.PlaybackLoudness/1000,0,0,16)
WEAPONGUI.Boxy.Rotation = WEAPONGUI.Boxy.Rotation+sick.PlaybackLoudness/125
WEAPONGUI.Vis.BackgroundColor3 = C3(math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1))
WEAPONGUI.Vis2.BackgroundColor3 = C3(math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1))
WEAPONGUI.Vis3.BackgroundColor3 = C3(math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1))
WEAPONGUI.Boxy.BackgroundColor3 = C3(math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1),math.min(sick.PlaybackLoudness/1000,1))
end
end))

while true do
	Swait()
	if AntiBullet == true then
if workspace:FindFirstChild("EnCorrupt") == nil then
Shield = Instance.new("MeshPart",workspace)
Shield.Name = "EnCorrupt"
Shield.CanCollide = false
Shield.Transparency = 1
Shield.Material = "Neon"
Shield.Color = Color3.fromRGB(val,val,val)
Shield.Size = Vector3.new(5,5,5)
Shield.CFrame = Torso.CFrame
local GlitchWeld = Instance.new("Weld",Shield)
GlitchWeld.Part0 = Shield
GlitchWeld.Part1 = Torso
end
	end
	if Character:FindFirstChild("Safety") == nil then
		local Safety = IT("Script")
		Safety.Name = "Safety"
		Safety.Parent = Character
	end
	for _,v in next, Humanoid:GetPlayingAnimationTracks() do
	    v:Stop();
	end
	SINE = SINE + CHANGE
	vissine = vissine + CHANGE * sick.PlaybackLoudness/150
	sine = sine + CHANGE
