                Humanoid = IT("Humanoid",Character)

            end
        end
    end
end

local BODY5 = {}
local EXTRATRANS = 0

function Refit6()
	for e = 1, #BODY5 do
		if BODY5[e] ~= nil then
			local STUFF = BODY5[e]
			local PART = STUFF[1]
			local PARENT = STUFF[2]
			local MATERIAL = STUFF[3]
			local COLOR = STUFF[4]
