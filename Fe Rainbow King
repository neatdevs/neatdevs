function modetoggle()
if mde == "Corrupt" then
Weapon.Parent = nil
		InsanityGun.Parent = nil
		feGun.Position = Vector3.new(0,0,0)
WEAPONGUI.Mode.mdename.Text = "Mode: Relaxed"
WEAPONGUI.uwu.uwu2.Text = "No Attacks Here"
mde = "Relaxed"
sick.SoundId = 'rbxassetid://709100631'
sick.PlaybackSpeed = 1
Speed = 40
elseif mde == "Relaxed" then
Weapon.Parent = nil
		InsanityGun.Parent = Character
		feGun.Position = Vector3.new(0,-1.4,-.5)
