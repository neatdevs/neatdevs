local function XZOFI_fake_script() -- Bronzemedal.LocalScript 
	local script = Instance.new('LocalScript', Bronzemedal)

	local emoji = script.Parent.Text

	local button = script.Parent

	button.MouseButton1Click:Connect(function()
		local args = {
			[1] = emoji,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end)
end
coroutine.wrap(XZOFI_fake_script)()
