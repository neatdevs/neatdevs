-- Scripts:

local function DPBFUT_fake_script() -- Thumbsup.LocalScript 
	local script = Instance.new('LocalScript', Thumbsup)

	local emoji = script.Parent.Text

	local button = script.Parent

	button.MouseButton1Click:Connect(function()
		local args = {
			[1] = emoji,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end)


end
coroutine.wrap(DPBFUT_fake_script)()
