close_2.Name = "close"
close_2.Parent = TopBar_2
close_2.BackgroundTransparency = 1.000
close_2.Position = UDim2.new(0.916356862, 0, 0, 0)
close_2.Size = UDim2.new(0, 22, 0, 23)
close_2.ZIndex = 2
close_2.Image = "rbxassetid://3926305904"
close_2.ImageRectOffset = Vector2.new(284, 4)
close_2.ImageRectSize = Vector2.new(24, 24)
close_2.MouseButton1Click:Connect(function()
	if toggle == true then
		ScrollingFrame:TweenSize(UDim2.new(0, 246,0, 0))
		ChatEmoji:TweenSize(UDim2.new(0, 269,0, 24))
		EmojiKeyboard:Destroy()
		toggle = false
	end
end)

remove_2.Name = "remove"
remove_2.Parent = TopBar_2
remove_2.BackgroundTransparency = 1.000
remove_2.LayoutOrder = 4
remove_2.Position = UDim2.new(0.830855012, 0, 2.23517418e-08, 0)
remove_2.Size = UDim2.new(0, 23, 0, 23)
remove_2.ZIndex = 2
remove_2.Image = "rbxassetid://3926307971"
remove_2.ImageRectOffset = Vector2.new(884, 284)
remove_2.ImageRectSize = Vector2.new(36, 36)
remove_2.MouseButton1Click:Connect(function()
	if toggle == true then
		ScrollingFrame:TweenSize(UDim2.new(0, 246,0, 0))
		ChatEmoji:TweenSize(UDim2.new(0, 269,0, 24))
		toggle = false
	elseif toggle == false then
		ScrollingFrame:TweenSize(UDim2.new(0, 246,0, 281))
		ChatEmoji:TweenSize(UDim2.new(0, 269,0, 305))
		toggle = true
	end
end)

