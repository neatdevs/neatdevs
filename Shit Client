AFK.MouseButton1Click:Connect(function()
	wait(500)

	for i,v in pairs(getconnections(game:GetService('Players').LocalPlayer.Idled)) do
		if v['Disable'] then
			v['Disable'](v)
		elseif v['Disconnect'] then
			v['Disconnect'](v)
		end
	end
end)

