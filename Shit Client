Zoom.Name = "Zoom"
Zoom.Parent = ZoomHolder
Zoom.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
Zoom.Position = UDim2.new(0, 0, 0.722222209, 0)
Zoom.Size = UDim2.new(0, 183, 0, 50)
Zoom.Font = Enum.Font.Gotham
Zoom.Text = "Enable"
Zoom.TextColor3 = Color3.fromRGB(255, 255, 255)
Zoom.TextSize = 30.000
Zoom.TextWrapped = true
Zoom.MouseButton1Click:Connect(function()
	local cam = game.Workspace.Camera
	local plr = game.Players.LocalPlayer

	local UserInt = game:GetService("UserInputService")

	UserInt.InputBegan:Connect(function(input, gameProcessed, IsTyping)

		if IsTyping then return end
		if input.KeyCode == Enum.KeyCode.C then
			cam.FieldOfView = 30
		end
	end)

	UserInt.InputEnded:Connect(function(input, gameProcessed, IsTyping)

		if IsTyping then return end
		if input.KeyCode == Enum.KeyCode.C then
			cam.FieldOfView = 70
		end
	end)
end)

