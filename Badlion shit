local function LJCQ_fake_script() -- LMB.LocalScript 
	local script = Instance.new('LocalScript', LMB)

	local Key = script.Parent
	local toggle = false
	local uis = game:GetService("UserInputService")

	uis.InputBegan:Connect(function(input, gameprocessedevent)
		if input.UserInputType==Enum.UserInputType.MouseButton1 then do
				wait()
				Key.TextColor3 = Color3.fromRGB(18, 18, 18)
				Key.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			end
		end

		wait(0.2)

		if input.UserInputType==Enum.UserInputType.MouseButton1 then do
				wait()
				Key.TextColor3 = Color3.fromRGB(225,225,225)
				Key.BackgroundColor3 = Color3.fromRGB(18, 18, 18)	
			end
		end
	end)

	
end
coroutine.wrap(LJCQ_fake_script)()
