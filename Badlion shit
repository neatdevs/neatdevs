-- Scripts:

local function VWJDJ_fake_script() -- W.LocalScript 
	local script = Instance.new('LocalScript', W)

	local Key = script.Parent
	local toggle = false
	local uis = game:GetService("UserInputService")

	uis.InputBegan:Connect(function(key, gp)
		task.spawn(function()
			while uis:IsKeyDown(Enum.KeyCode.W) do
				Key.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Key.TextColor3 = Color3.fromRGB(18, 18, 18)
				wait()
			end
			Key.TextColor3 = Color3.fromRGB(225,225,225)
			Key.BackgroundColor3 = Color3.fromRGB(18,18,18)
		end)
	end)
	
end
coroutine.wrap(VWJDJ_fake_script)()
